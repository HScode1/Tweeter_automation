    "lucide-react": "^0.474.0",
    "motion": "^12.4.0",
    "next": "15.1.6",
    "next-auth": "^4.24.11",
    "openai": "^4.83.0",
    "prisma": "^6.3.1",
    "react": "^18.0.0",
    "react-day-picker": "^9.5.1",
    "react-dom": "^18.0.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "twitter-api-v2": "^1.20.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
